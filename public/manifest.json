{
  "manifest_version": 3,
  "name": "RT Upbit price",
  "version": "1.0",
  "description": "real-time upbit coin price",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://api.upbit.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
